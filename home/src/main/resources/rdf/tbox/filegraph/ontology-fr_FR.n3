ehess:Relation a owl:Class ;
	rdfs:label "Relation"@fr-FR;
	owl:equivalentClass vivo:Relationship ;
	dcterms:description	"Relation réifiée entre deux entités"@fr-FR .

ehess:Position a owl:Class ;
        owl:equivalentClass vivo:Position ;
	dcterms:description	"Une position dans une organisation"@fr-FR ;
        rdfs:subClassOf ehess:Relation ;
        rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty vivo:relates ;
	        owl:someValuesFrom <http://xmlns.com/foaf/0.1/Person>
              ] ;
        rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty vivo:relates ;
	        owl:someValuesFrom <http://xmlns.com/foaf/0.1/Organization>
              ] ;
        rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty vivo:rank ;
	        owl:someValuesFrom <http://www.w3.org/2001/XMLSchema#int>
              ] ;
        rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty vivo:dateTimeInterval ;
	        owl:allValuesFrom vivo:DateTimeInterval
              ] .

