ehess:  a                          owl:Ontology ;
        rdfs:label                 "EHESS Ontology"@en-US ;
        rdfs:label                 "Ontologie EHESS"@fr-FR ;
        vitro:ontologyPrefixAnnot  "ehess"^^xsd:string ;
	dcterms:description "Ontologie décrivant l'ensemble de personnel et des structures de recherche à l'Ecole des hautes études en sciences sociales (EHESS), la plupart des concepts sont compatibles avec le fonctionnement d'un établissement ESR"@fr-FR;
	dcterms:creator <http://data.ehess.fr/person/jdornbusch>, <http://data.ehess.fr/person/cbrando> ;
	dcterms:issued "2018-04-03"^^xsd:date ;
	dcterms:rights "CC BY-SA 4.0" ;
	cc:license <https://creativecommons.org/licenses/by-sa/4.0/> ;
	vann:preferredNamespacePrefix "ehess" ;
	vann:preferredNamespaceUri <http://data.ehess.fr/ontology#> ;
        owl:versionInfo            "Version 1.0 - 2018-04-03" ;
        owl:imports <http://vivoweb.org/ontology/core#> .

